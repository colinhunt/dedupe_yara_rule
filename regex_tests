# Regular Expression Tests

variable: yara_rule_regex

1. r"(^rule\s.*?\scondition.*?\s\})"  # - experiment 1
2. r"(^ ?rule\s[0-9a-zA-Z_\@\#\$\%\^\&\(\)\-\=\:\s]+\{.*?condition.*?\s\})"  # - experiment 2
3. r"(^[\s+]*rule\s[0-9a-zA-Z_\@\#\$\%\^\&\(\)\-\=\:\s]+\{.*?condition.*?\s\})" - experiment 3

